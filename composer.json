{
    "name": "horde/pack",
    "description": "Data packing library",
    "type": "library",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "version": "1.0.8",
    "time": "2017-11-11",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.3 || ^7",
        "pear-pear.horde.org/Horde_Compress_Fast": "^1",
        "pear-pear.horde.org/Horde_Exception": "^2"
    },
    "suggest": {
        "pear-pear.horde.org/Horde_Test": "^2",
        "pear-pecl.php.net/igbinary": "^1.2",
        "pear-pecl.php.net/msgpack": "*",
        "ext-json": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Pack": "1.*",
        "pear-horde/Horde_Pack": "1.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Pack": "lib/"
        }
    }
}
