{
    "minimum-stability": "dev",
    "name": "horde/pack",
    "description": "Data packing library",
    "type": "library",
    "homepage": "https://www.horde.org",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Michael Slusarz",
            "email": "slusarz@horde.org",
            "role": "lead"
        }
    ],
    "time": "2022-11-04",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/compress_fast": "^2 || dev-FRAMEWORK_6_0",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {},
    "suggest": {
        "horde/test": "^3 || dev-FRAMEWORK_6_0",
        "ext-igbinary": "^1.2",
        "ext-msgpack": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Pack": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Pack\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}